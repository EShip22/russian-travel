.place__paragraph {
	margin: 0;
	font-size: 18px;
	line-height: 29.34px;
	font-style: normal;
	font-weight: 400;
}
.place__paragraph:nth-child(1) {
	margin-bottom: 26px;
}

@media screen and (min-width:320px) and (max-width:1023px) {
	.place__paragraph {
		font-size: 16px;
		line-height: 29.34px;
	}
}